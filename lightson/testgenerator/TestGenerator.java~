import java.util.Random;

public class TestGenerator {
    

    public static void main(String[] args) {
        int size = args[0];
        Random rand = new Random();

        if (args[0] == null) {
            size = 20;
        }

        for (int i = 0; i < size; i++) {
            char c = (char) (rand.nextInt % 26) + 48
        }
        
}
